{
  "name": "frontend",
  "version": "0.0.1",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm-run-all compile",
    "compile": "npm-run-all compile:typescript compile:sass",
    "compile:typescript": "tsc --build",
    "compile:sass": "webpack -p --config webpack.config.sass.ts",
    "clean": "rm -rf node_modules/ && rm -rf lib/ && rm -rf dist/",
    "serve": "http-server dist/",
    "test": "jest --config=jest.config.js --all",
    "test:updateSnapshot": "jest --config=jest.config.js --updateSnapshot"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.23.1",
    "@blueprintjs/datetime": "^3.15.2",
    "@blueprintjs/icons": "^3.13.0",
    "@blueprintjs/select": "^3.11.2",
    "@blueprintjs/table": "^3.8.3",
    "@blueprintjs/timezone": "^3.6.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.41.5",
    "css-loader": "^3.4.2",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
